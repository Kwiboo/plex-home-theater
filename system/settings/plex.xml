<?xml version="1.0" encoding="utf-8" ?>
<settings>
  <section id="music">
    <category id="musicplayer" label="14086" help="36264">
      <group id="4">
        <setting id="musicplayer.visualisation">
          <default>visualization.nowplaying</default>
        </setting>
      </group>
    </category>
  </section>
  <section id="system">
    <category id="plex" label="" help="">
      <group id="1">
        <setting id="system.uuid" type="string" label="99999" help="99999">
          <level>3</level>
          <default></default>
          <constraints>
            <allowempty>true</allowempty>
          </constraints>
          <control type="edit" format="string" />
        </setting>
        <setting id="system.firstrunwizard" type="boolean" label="99999" help="99999">
          <level>3</level>
          <default>false</default>
          <control type="toggle" />
        </setting>
        <setting id="system.mostrecentplayqueue" type="string" label="99999" help="99999">
          <level>3</level>
          <default></default>
          <constraints>
            <allowempty>true</allowempty>
          </constraints>
          <control type="edit" format="string" />
        </setting>
      </group>
    </category>
    <category id="myplex" label="10" help="10">
      <group id="1">
        <setting id="myplex.uid" type="string" label="99999" help="99999">
          <level>3</level>
          <default></default>
          <constraints>
            <allowempty>true</allowempty>
          </constraints>
          <control type="edit" format="string" />
        </setting>
        <setting id="myplex.token" type="string" label="99999" help="99999">
          <level>3</level>
          <default></default>
          <constraints>
            <allowempty>true</allowempty>
          </constraints>
          <control type="edit" format="string" />
        </setting>
        <setting id="myplex.status" type="string" label="15000" help="15000">
          <level>3</level>
          <default></default>
          <constraints>
            <allowempty>true</allowempty>
          </constraints>
          <control type="edit" format="string" />
        </setting>
        <setting id="myplex.signin" type="action" label="44100" help="44100">
          <level>3</level>
          <control type="button" format="action" />
        </setting>
        <setting id="myplex.searchsharedlibraries" type="boolean" label="13143" help="13143">
          <level>0</level>
          <default>true</default>
          <control type="toggle" />
        </setting>
        <setting id="myplex.enablequeueandrec" type="boolean" label="52210" help="52210">
          <level>0</level>
          <default>true</default>
          <control type="toggle" />
        </setting>
        <setting id="myplex.hidecloudsync" type="boolean" label="52505" help="52505">
          <level>0</level>
          <default>false</default>
          <control type="toggle" />
