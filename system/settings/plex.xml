  <section id="videos">
    <category id="videoacceleration">
      <group id="4">
        <setting id="videoplayer.useffmpegavio" type="boolean" label="44406" help="44406">
          <level>0</level>
          <default>true</default>
          <control type="toggle" />
        </setting>
      </group>
    </category>
    <category id="extras" label="44408" help="44408">
      <group id="1">
        <setting id="videoplayer.playtrailercount" type="integer" label="44407" help="44407">
          <level>0</level>
          <default>0</default>
          <constraints>
            <minimum>0</minimum>
            <step>1</step>
            <maximum>5</maximum>
          </constraints>
          <control type="spinner" format="integer" />
        </setting>
      </group>
    </category>
  </section>
  <section id="services">
    <category id="plexmediaserver" label="40210" help="40210">
      <group id="1">
        <setting id="plexmediaserver.manualaddress" type="boolean" label="40211" help="40211">
          <level>0</level>
          <default>false</default>
          <control type="toggle" />
        </setting>
        <setting id="plexmediaserver.address" type="string" label="40212" help="40212">
          <level>0</level>
          <default>0.0.0.0</default>
          <dependencies>
            <dependency type="enable" setting="plexmediaserver.manualaddress">true</dependency>
          </dependencies>
          <control type="edit" format="ip" />
        </setting>
        <setting id="plexmediaserver.port" type="integer" label="792" help="792">
          <level>0</level>
          <default>32400</default>
          <constraints>
            <minimum>1</minimum>
            <step>1</step>
            <maximum>65535</maximum>
          </constraints>
          <dependencies>
            <dependency type="enable" setting="plexmediaserver.manualaddress">true</dependency>
          </dependencies>
          <control type="edit" format="integer" />
        </setting>
		
        <setting id="services.plexplayer" type="boolean" label="44300" help="44300">
          <level>0</level>
          <default>true</default>
          <control type="toggle" />
        </setting>
      </group>
    </category>
  </section>
</settings>
